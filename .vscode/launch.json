{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "rundb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rundb",
            "args": ["-nid1",">","db1.out"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "runcl",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/runcl",
            "args": ["-nid3",">","cl3.out"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}